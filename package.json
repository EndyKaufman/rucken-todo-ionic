{
  "name": "rucken-todo-ionicframework",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "http://ionicframework.com/",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "lint": "ionic-app-scripts lint",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "tools-demo-add_version": "gulp add-version",
    "tools-build": "npm-run-all demo-build tools-demo-add_version",
    "tools-extract_translate": "ngx-translate-extract --input ./src/app/todo --output ./src/app/todo/i18n/template.pot --format=pot --marker translate --clean",
    "tools-po2ts": "srcgen -x -t convert.po.to.ts -f ./srcgen/convert.po.to.ts.json",
    "tools-make_ts_list": "srcgen -x -t make.list.ts.files -f ./srcgen/make.list.ts.files.json",
    "tools-prepare": "npm-run-all tools-extract_translate tools-po2ts tools-make_ts_list"
  },
  "dependencies": {
    "@angular/common": "4.4.5",
    "@angular/compiler": "4.4.5",
    "@angular/compiler-cli": "4.4.5",
    "@angular/core": "4.4.5",
    "@angular/forms": "4.4.5",
    "@angular/http": "4.4.5",
    "@angular/platform-browser": "4.4.5",
    "@angular/platform-browser-dynamic": "4.4.5",
    "@ionic-native/core": "4.4.0",
    "@ionic-native/splash-screen": "4.4.0",
    "@ionic-native/status-bar": "4.4.0",
    "@ionic/storage": "2.1.3",
    "@ngx-translate/core": "^8.0.0",
    "@rucken/core": "^1.25.1",
    "@types/jasmine": "2.8.0",
    "@types/lodash": "^4.14.85",
    "@types/marked": "0.3.0",
    "@types/node": "^8.0.51",
    "@types/requirejs": "^2.1.31",
    "@types/webpack": "^3.8.1",
    "angular2-jwt": "^0.2.3",
    "ionic-angular": "3.9.2",
    "ionicons": "3.0.0",
    "lodash": "^4.17.4",
    "moment": "^2.19.2",
    "rxjs": "5.5.2",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@ionic/app-scripts": "3.1.2",
    "srcgen": "^0.3.0",
    "typescript": "2.4.2",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "conventional-changelog-cli": "^1.3.5",
    "conventional-commits-detector": "^0.1.1",
    "conventional-recommended-bump": "^1.0.3",
    "gh-pages": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-replace": "^0.6.1",
    "gulp-tsc": "^1.3.2",
    "markdown-loader": "^2.0.1",
    "npm-run-all": "^4.1.2",
    "po2json": "^0.4.5",
    "recursive-readdir": "^2.2.1",
    "replace-ext": "^1.0.0"
  },
  "description": "An Ionic project"
}
